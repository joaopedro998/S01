REM ENTRANDO COM VALORES 
10 INPUT "Digite o primeiro número:"; NUM1 
20 INPUT "Digite o segundo número:"; NUM2 
30 INPUT "Digite o terceiro número:"; NUM3 

REM FAZENDO A OPERAÇAO
40 LET AUX = NUM2^2 - 4 * NUM1 * NUM3
50 IF AUX < 0 THEN PRINT "NÃO EXISTEM RAÍZES REAIS" GOTO 100
60 LET RAIZ1 = (-NUM2 + SQR(AUX)) / (2 * NUM1)
70 LET RAIZ2 = (-NUM2 - SQR(AUX)) / (2 * NUM1)
80 PRINT RAIZ1
90 PRINT RAIZ2

100 END

REM 2

10 REM GERA UM NUMERO ALEATORIO 
20 RANDOMIZE TIMER
30 LET NUM = INT(RND * 10)
40 PRINT NUM

50 LET FATORIAL = 1
60 FOR I = 1 TO NUM
70 LET FATORIAL = FATORIAL * I
80 NEXT I
90 PRINT FATORIAL

100 END
